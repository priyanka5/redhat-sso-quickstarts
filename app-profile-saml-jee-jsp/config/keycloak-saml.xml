<keycloak-saml-adapter>
    <SP entityID="app-profile-saml"
        sslPolicy="EXTERNAL"
        logoutPage="SPECIFY YOUR LOGOUT PAGE!">
        <Keys>
            <Key signing="true">
                <PrivateKeyPem>
                    
                </PrivateKeyPem>
                <CertificatePem>
                    
                </CertificatePem>
            </Key>
        </Keys>
        <IDP entityID="idp"
             signatureAlgorithm="RSA_SHA256"
             signatureCanonicalizationMethod="http://www.w3.org/2001/10/xml-exc-c14n#">
            <SingleSignOnService signRequest="true"
                                 validateResponseSignature="true"
                                 validateAssertionSignature="false"
                                 requestBinding="POST"
                                 bindingUrl="https://sso-single-sign-on.lab.ocp.azure.rai.it/auth/realms/demo/protocol/saml"/>
            <SingleLogoutService signRequest="true"
                                 signResponse="true"
                                 validateRequestSignature="true"
                                 validateResponseSignature="true"
                                 requestBinding="POST"
                                 responseBinding="POST"
                                 postBindingUrl="https://sso-single-sign-on.lab.ocp.azure.rai.it/auth/realms/demo/protocol/saml"
                                 redirectBindingUrl="https://sso-single-sign-on.lab.ocp.azure.rai.it/auth/realms/demo/protocol/saml"/>
        </IDP>
    </SP>
</keycloak-saml-adapter>
